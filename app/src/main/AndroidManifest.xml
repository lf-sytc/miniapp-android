<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uhutu.miniapp.miniapp_android">

    <application
        android:name="com.alipay.mobile.framework.quinoxless.QuinoxlessApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".SubActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="mpaas.quinoxless.extern.application"
            android:value="com.uhutu.miniapp.miniapp_android.MyApplication"
            />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>